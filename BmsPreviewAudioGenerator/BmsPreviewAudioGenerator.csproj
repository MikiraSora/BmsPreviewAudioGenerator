<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFrameworks>net8.0</TargetFrameworks>
		<Platforms>AnyCPU;x86;x64</Platforms>
		<Version>0.9.9.7</Version>
